      <Label fx:id="TIISCDLabel" layoutX="527.0" layoutY="438.0" prefHeight="16.0" prefWidth="113.0" text="0.00" />
      <Label fx:id="FinalTotalLabel" layoutX="524.0" layoutY="480.0" prefHeight="16.0" prefWidth="113.0" text="0.00">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
